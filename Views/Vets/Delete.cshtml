@model ao188116_MIS4200.Models.Vet

@{
    ViewBag.Title = "Delete  Vet";
}

<h2>Delete Selected Vet</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Vet</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Vet Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.vFullName)
        </dd>



    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            @Html.ActionLink("Back to List of Vets", "Index")
        </div>
    }
</div>
